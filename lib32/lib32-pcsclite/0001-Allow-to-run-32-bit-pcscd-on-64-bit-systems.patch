Subject: [PATCH] Allow to run 32-bit pcscd on 64-bit systems

---
 etc/pcscd.service.in | 1 -
 1 file changed, 1 deletion(-)

diff --git a/etc/pcscd.service.in b/etc/pcscd.service.in
index 60732c17..b3ad500d 100644
--- a/etc/pcscd.service.in
+++ b/etc/pcscd.service.in
@@ -46,7 +46,6 @@ RestrictSUIDSGID=yes
 # System Call Filtering
 SystemCallFilter=@system-service
 SystemCallFilter=~@resources @privileged
-SystemCallArchitectures=native
 
 [Install]
 Also=pcscd@program_suffix@.socket
